{
  "source": {
    "include": ["./"],
    "exclude": ["node_modules", "docs", "eslint.config.js"],
    "includePattern": ".+\\.js(doc|x)?$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
